{
	"name": "cheat-sheet",
	"version": "1.0.0",
	"description": "Albert's cheat sheet",
	"type": "module",
	"scripts": {
		"dev": "rspress dev -c configs/base.ts --port 5007",
		"dev:tools": "rspress dev -c configs/tools.ts --port 5004",
		"dev:backend": "rspress dev -c configs/backend.ts --port 5006",
		"dev:web": "rspress dev -c configs/web.ts --port 5005",
		"dev:se": "rspress dev -c configs/se.ts --port 5001",
		"dev:devops": "rspress dev -c configs/devops.ts --port 5003",
		"dev:de": "rspress dev -c configs/de.ts --port 5002",
		"build": "rspress build",
		"preview": "rspress preview --port 5002",
		"lint": "ls docs/**/*.mdx | node scripts/lint-markdown.js",
		"prepare": "node .husky/install.mjs"
	},
	"dependencies": {
		"rspress": "^1.40.2",
		"rspress-plugin-katex": "^0.3.0",
		"rspress-plugin-mermaid": "^1.0.0"
	},
	"devDependencies": {
		"@types/node": "^18.11.17",
		"husky": "^9.1.7",
		"lint-staged": "^16.1.2",
		"prettier": "^3.8.0"
	},
	"lint-staged": {
		"*.mdx": "node scripts/lint-markdown.js"
	}
}
