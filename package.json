{
	"name": "cheat-sheet",
	"version": "1.0.0",
	"description": "Albert's cheat sheet",
	"type": "module",
	"scripts": {
		"dev": "rspress dev --port 5002",
		"build": "rspress build",
		"preview": "rspress preview --port 5002",
		"lint": "find pages -name \\*.md -print0 | tr '\\0' '\\n' | node lint-mdx.js",
    "prepare": "node .husky/install.js"
	},
	"dependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"rspress-plugin-katex": "^0.3.0"
	},
	"devDependencies": {
		"lint-staged": "^16.1.2",
		"markdown-link-check": "3.12.2",
		"rspress": "^1.42.0",
		"typescript": "5.8.2",
		"husky": "^8.0.0"
	},
	"lint-staged": {
		"*.mdx": "npx markdown-link-check"
	}
}
